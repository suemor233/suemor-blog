    "next": "12.3.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "randomcolor": "^0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
